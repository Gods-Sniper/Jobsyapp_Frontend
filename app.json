{
  "expo": {
    "name": "Jobsy",
    "slug": "Jobsy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo3.png",
    "scheme": "jobsy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo3.png",
        "backgroundColor": "#40189D"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.Jobsy"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo3.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo4.png",
          "imageWidth": 300,
          "resizeMode": "contain",
          "backgroundColor": "#40189D"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
